{
  "name": "typst-figure-gallery",
  "displayName": "typst-figure-gallery",
  "description": "Show images from active Typst file",
  "version": "0.0.2",
  "publisher": "lamininA1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamininA1/typst-figure-gallery"
  },
  "engines": {
    "vscode": "^1.90.0" 
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "typst",
    "figure",
    "gallery",
    "image"
  ],
  "activationEvents": [
    "onLanguage:typst"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typst-figure-gallery.openGallery",
        "title": "Typst: Open Figure Gallery"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "typescript-eslint": "^8.18.1",
    "eslint": "^9.17.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}